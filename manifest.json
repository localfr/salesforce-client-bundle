{
    "bundles": {
        "Localfr\\SalesforceClientBundle\\LocalfrSalesforceClientBundle": ["all"]
    },
    "copy-from-package": {
        "config/": "%CONFIG_DIR%/"
    },
    "env": {
        "LOCALFR_SALESFORCE_CLIENT_ID": "<Salesforce client_id>",
        "LOCALFR_SALESFORCE_CLIENT_SECRET": "<Salesforce client_secret>",
        "LOCALFR_SALESFORCE_PRIVATE_KEY": "<Salesforce private key>",
        "LOCALFR_SALESFORCE_PUBLIC_KEY": "<Salesforce public key>",
        "LOCALFR_SALESFORCE_USERNAME": "<Salesforce username>"
    }
}
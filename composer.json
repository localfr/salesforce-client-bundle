{
    "name": "localfr/salesforce-client-bundle",
    "type": "symfony-bundle",
    "description": "A Symfony Bundle for Salesforce",
    "homepage": "https://github.com/localfr/salesforce-client-bundle",
    "keywords": ["salesforce", "symfony", "bundle"],
    "license": "MIT",
    "authors": [
        {
            "name": "local.fr",
            "email": "developpeurs-web@local.fr"
        }
    ],
    "require": {
        "php": ">=7.1",
        "symfony/framework-bundle": "^5.3",
        "lcobucci/jwt": "^4.1",
        "symfony/http-client": "^5.3",
        "symfony/cache": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "Localfr\\SalesforceClientBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Localfr\\SalesforceClientBundle\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "friendsofphp/php-cs-fixer": "^3.3",
        "symfony/yaml": "^5.3"
    }
}
